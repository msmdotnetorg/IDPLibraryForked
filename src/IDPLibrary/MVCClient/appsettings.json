{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenIdConnect": {
    "Authority": "https://localhost:7085",
    "ClientId": "mvcclient",
    "ClientSecret": "MVCClientSecret",
    "ResponseType": "code",
    "ResponseMode": "query",
    "CallbackPath": "/signin-oidcCallback",
    "Scope": [ "openid", "profile", "email" ],
    "TokenValidationParameters": {
      "ValidateIssuerSigningKey": false
    }
  }
}
